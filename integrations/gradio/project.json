{
  "name": "gradio",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "integrations/gradio",
  "tags": ["type:integration", "lang:python"],
  "targets": {
    "serve": {
      "executor": "@nxlv/python:run",
      "options": {
        "module": "app",
        "cwd": "integrations/gradio"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'No tests implemented for Gradio integration yet'",
        "cwd": "integrations/gradio"
      }
    },
    "lint": {
      "executor": "@nxlv/python:lint",
      "options": {
        "cwd": "integrations/gradio",
        "lintFilePatterns": ["**/*.py"]
      }
    },
    "install": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pip install -r requirements.txt",
        "cwd": "integrations/gradio"
      }
    }
  }
}