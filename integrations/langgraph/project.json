{
  "name": "langgraph",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "integrations/langgraph/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/integrations/langgraph/dist"],
      "options": {
        "commands": [
          "mkdir -p integrations/langgraph/dist",
          "echo 'module.exports = require(\"../src/index\");' > integrations/langgraph/dist/index.js",
          "echo 'LangGraph build skipped due to memory constraints'"
        ],
        "parallel": false
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "integrations/langgraph/jest.config.cjs",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["integrations/langgraph/**/*.ts"]
      }
    }
  },
  "tags": ["integration", "langgraph"]
}