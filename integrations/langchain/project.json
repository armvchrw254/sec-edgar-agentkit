{
  "name": "langchain",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "integrations/langchain",
  "tags": ["type:integration", "lang:typescript"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "integrations/langchain/dist",
        "main": "integrations/langchain/index.ts",
        "tsConfig": "integrations/langchain/tsconfig.json",
        "assets": [
          "integrations/langchain/README.md",
          "integrations/langchain/LICENSE",
          "integrations/langchain/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "integrations/langchain/jest.config.cjs",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["integrations/langchain/**/*.ts"]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm publish",
        "cwd": "integrations/langchain"
      },
      "dependsOn": ["build"]
    }
  }
}