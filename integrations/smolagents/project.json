{
  "name": "smolagents",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "integrations/smolagents",
  "tags": ["type:integration", "lang:python"],
  "targets": {
    "lint": {
      "executor": "@nxlv/python:ruff-check",
      "outputs": [],
      "options": {
        "outputFile": "reports/integrations/smolagents/ruff.txt"
      }
    },
    "test": {
      "executor": "@nxlv/python:pytest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "testFiles": ["integrations/smolagents/__tests__"],
        "coverage": true,
        "coverageDirectory": "coverage/integrations/smolagents"
      }
    },
    "build": {
      "executor": "@nxlv/python:poetry-build",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "outputPath": "integrations/smolagents/dist"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "python examples/basic_usage.py",
        "cwd": "integrations/smolagents"
      }
    },
    "publish": {
      "executor": "@nxlv/python:poetry-publish",
      "options": {
        "repository": "pypi"
      },
      "dependsOn": ["build"]
    }
  }
}