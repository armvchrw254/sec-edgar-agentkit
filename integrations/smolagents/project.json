{
  "name": "smolagents",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "integrations/smolagents",
  "tags": ["type:integration", "lang:python"],
  "targets": {
    "lint": {
      "executor": "@nxlv/python:ruff-check",
      "outputs": [],
      "options": {
        "outputFile": "reports/integrations/smolagents/ruff.txt"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'No tests implemented for smolagents integration yet'",
        "cwd": "integrations/smolagents"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pip install build && python -m build || echo 'Build requires Python dependencies'",
        "cwd": "integrations/smolagents"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "python examples/basic_usage.py",
        "cwd": "integrations/smolagents"
      }
    },
    "publish": {
      "executor": "@nxlv/python:publish",
      "options": {
        "repository": "pypi"
      },
      "dependsOn": ["build"]
    }
  }
}